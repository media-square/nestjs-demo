{
  "name": "nestjs-demo-app",
  "version": "1.0.0",
  "description": "",
  "author": "Martijn van Waardhuizen",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "docker:build": "docker-compose -f ./docker/services.yaml -p local_dev build",
    "docker:build:test": "docker-compose -f ./docker/services-testing.yaml -p local_testing build",
    "docker:up": "docker-compose -f ./docker/services.yaml -p local_dev up",
    "docker:up:test": "docker-compose -f ./docker/services-testing.yaml -p local_testing up",
    "docker:down": "docker-compose -f ./docker/services.yaml -p local_dev down --volumes",
    "docker:down:test": "docker-compose -f ./docker/services-testing.yaml -p local_testing down --volumes",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node --test --require ts-node/register \"./__tests__/**/*.test.ts\""
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^8.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@aws-sdk/client-s3": "^3.717.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "minio": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "passport-custom": "^1.1.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "eslintConfig": {
    "extends": "airbnb"
  }
}
